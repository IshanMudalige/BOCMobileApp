<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aim.bocmobileapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".ForgotPasswordNew"
            android:label="Select New Password"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".ForgotPassword"
            android:label="Forgot Password"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".LogIn"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApp.No">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogInPage"
            android:label="Login"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp"/>
        <activity
            android:name=".RegisterPage"
            android:label="Register"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".PayBills"
            android:label="Pay Bills"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".OwnAccount"
            android:label="Own Account Transfer"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".BillerUpdateActivity"
            android:label="Biller Maintenance"
            android:parentActivityName=".BillerMaintainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".BillerMaintainActivity"
            android:label="Biller Maintenance"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".TransactionSuccessActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".ThirdpartyTransferActivity"
            android:label="Third party Transfer"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".PayeeUpdateActivity"
            android:label="Payee Maintenance"
            android:parentActivityName=".PayeeMaintainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".PayeeMaintainActivity"
            android:label="Payee Maintenance"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.MyApp" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>